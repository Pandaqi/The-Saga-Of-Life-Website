@font-face{font-family:gentium book plus;src:url(GentiumBookPlus-Italic.woff2)format("woff2");font-weight:400;font-style:italic;font-display:swap}@font-face{font-family:gentium book plus;src:url(GentiumBookPlus.woff2)format("woff2");font-weight:400;font-style:normal;font-display:swap}@font-face{font-family:gentium book plus;src:url(GentiumBookPlus-BoldItalic.woff2)format("woff2");font-weight:700;font-style:italic;font-display:swap}@font-face{font-family:gentium book plus;src:url(GentiumBookPlus-Bold.woff2)format("woff2");font-weight:700;font-style:normal;font-display:swap}@font-face{font-family:camelot caps;src:url(camelot_caps.woff2)format("woff2");font-weight:400;font-style:normal;font-display:swap}@page{size:5.5in 8.5in;margin-top:18mm;margin-bottom:22mm;@top-right{content: string(title);
    font-style: italic;
    opacity: 0.5;
  }@bottom-center{content: counter(page);
  }}@page:left{margin-left:15mm;margin-right:20mm}@page:right{margin-left:20mm;margin-right:15mm}*{box-sizing:border-box}body{font-size:11.55pt;background-color:#fff;line-height:1.4em;color:#000;font-family:gentium book plus}h1,h2,h3,h4,h5,h6{font-family:gentium book plus;font-weight:700}h1{font-size:2.3em}h2{page-break-before:always;margin-top:4em;font-size:1.75em;margin-bottom:.5em}p{margin:0}p+p{text-indent:1.5em}hr{margin-top:5em;margin-bottom:.5em;border-top:2px solid rgba(0,0,0,.2)}.content p:first-child::first-letter{float:left;font-family:camelot caps,Georgia,serif;display:block;font-style:normal;font-weight:400;color:#000;font-size:7.75em;line-height:.86em}